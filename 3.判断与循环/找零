#include <stdio.h>

int main(int argc, char **argv)
{
    // 初始化
    int price = 0;
    int bill = 0;
    // 读入金额和票面
    printf("请输入金额：");
    scanf("%d", &price);
    printf("请输入票面：");
    scanf("%d", &bill);
    // 计算找零
    printf("应该找您：%d\n", bill - price);
    return 0;
}
/* 注释块
    printf("%d", c*100+b*10+a);    // 注释
注释到此 */
